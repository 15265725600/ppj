/* pages/search/index.wxss */
.search-warp{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  padding: 0 25rpx;
  height: 90rpx;
}
.search-box{
  flex: 1;
  display: flex;
  align-items: center;
  border-radius: 60rpx;
  background: #f1f1f1;
  padding: 5rpx 30rpx;
}
icon{
  margin-right: 10rpx;
}
.search-box input{
  flex: 1;
}
.search-warp button{
  width: 30px;
  margin-left: 30rpx;
  font-size: 14px;
  background: #fff;
  border: none;
  padding: 0;
}
.list{
  padding: 0 25rpx;
}
.title{
  line-height: 80rpx;
  font-size: 15px;
}
.search-list{
  display: flex;
  margin-bottom: 30rpx;
}
.search-item{
  background: #e1e1e1;
  color: #666666;
  size: 12px;
  border-radius: 60rpx;
  line-height: 60rpx;
  margin-right: 20rpx;
  text-align: center;
  padding: 0 25rpx;
  display: inline-block;
  
}
.search-res{
  position: absolute;
  background: #fff;
  top: 110rpx;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100;
}
.search-res-item{
  line-height: 80rpx;
  border-bottom: 1px solid #e5e5e5;
}