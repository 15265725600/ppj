/* pages/recharge/index.wxss */
.recharge-title{
  line-height: 95rpx;
  padding: 0 35rpx;
  background: #fff;
  border-bottom: 1px solid #e5e5e5;
  border-top: 1px solid #e5e5e5;
  font-size: 15px;
}
.recharge-number{
  background: #fff;
  padding: 20rpx 35rpx;
}
.pay-way{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  margin-top: 15rpx;
  line-height: 95rpx;
  padding: 0 35rpx;
  font-size: 15px;
}
.pay-way .title{
  display: flex;
  align-items: center;
  justify-content: center;
}
.pay-way .icon{
  background-image: url(data:image/png;base64,);
  background-size: cover;
  width: 52rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.submit{
  margin-top: 40rpx;
}
.submit button{
  background: #fe5656;
  color: #fff;
  font-size: 15px;
  line-height: 80rpx;
  border-radius: 0;
}