/* pages/addr/index.wxss */
/* 地址 */
.list{
  margin-bottom: 80rpx;
}
.addr-item{
  background: #fff;
  margin-bottom: 10rpx;
}
.addr-item .top{
  padding:35rpx;
  border-bottom: 1px solid #e5e5e5;
}
.addr-item .bottom{
  padding: 0 35rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 80rpx;
}

.addr-item .select{
  font-size: 13px;
  display: flex;
  align-items: center;
}
.addr-item .select icon{
  margin-right: 10rpx;
}
.addr-item .addr-handle{
  display: flex;
}
.addr-item .edit .icon{
  width: 31rpx;
  height: 33rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  margin-right: 5rpx;
}
.addr-item .del .icon{
  width: 31rpx;
  height: 33rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  margin-right: 5rpx;
}
.addr-item .edit,.addr-item .del{
  margin-left: 60rpx;
  display: flex;
  align-items: center;
}
.addr-item .user{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 15px;
}
.addr-item .addr-detail{
  padding: 15rpx 0;
  font-size: 13px;
  color: #777777;
}
.submit{
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
}