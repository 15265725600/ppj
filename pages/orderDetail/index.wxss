/* pages/orderDetail/index.wxss */
.order-detail{
  margin-bottom: 105rpx;
}
/* 订单地址 */
.order-addr{
  display: flex;
  align-items: center;
  padding: 35rpx 40rpx;
  background: #fff;
  border-bottom: 2px dashed #facd89;
}
.order-addr .icon{
  width: 34rpx;
  height: 38rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  margin-right: 25rpx;
}
.order-addr .addr-detail{
  flex: 1;
}
.order-addr .user{
  font-size: 15px;
}
.order-addr .addr{
  color: #666;
  font-size: 13px;
  margin: 5rpx 0;
}


/* s商品 */
.shop-list{
  margin-top: 15rpx;
  background: #fff;
}


.order-detail-list{
  margin-top: 10rpx;
}
.order-detail-item{
  display: flex;
  align-items: center;
  padding: 0 35rpx;
  line-height: 95rpx;
  background: #fff;
  border-bottom: 1px solid #efefef;
}
.order-detail-item .price{
  font-size: 15px;
  color: #fe5656;
  font-weight: bold;
}
.order-detail-item .feight{
  font-size: 13px;
  color: #838383;
}
.order-info text{
  margin-left: 15rpx;
  font-size: 13px;
  color: #838383;
}
.shop-info .order-detail-item{
  justify-content: space-between;
}
.order-detail-handle button{
  color:#fe5656;
  border:1px solid #fe5656;
  background:#fff;
}
.order-detail-handle button:last-of-type{
  color:#fff;
  background:#fe5656;
  border:1px solid #fe5656;
}