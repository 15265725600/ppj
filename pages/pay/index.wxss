/* pages/pay/index.wxss */
.pay-item{
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  justify-content:  space-between;
  height: 95rpx;
  background: #fff;
  padding: 0 30rpx;
  font-size: 15px;
}
.name{
  display: flex;
}
.name text{
  font-size: 16px;
  color: #fe5656;
  margin-left: 10rpx;
}
.name .icon{
  background-image: url(data:image/png;base64,);
  background-size: cover;
  width: 52rpx;
  height: 44rpx;
  margin-right: 10rpx;
}

.mark{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.75);
  z-index: 100;
}
.ps-box .ps-warn{
  font-size: 17px;
  text-align: center;
  padding-top: 30rpx;
  color: #727272;
}
.ps-box{
  position: absolute;
  top: 35%;
  left: 50%;
  width: 80%;
  background: #fff;
  transform: translate(-50%,-50%);
  border-radius: 5px;
}
.pay-ps-box{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 30rpx 50rpx 80rpx 50rpx;
}
.pay-ps-box input{
  flex: 1;
  height: 80rpx;
  border: 1px solid #e5e5e5;
  border-right: none;
  font-size: 35px;
  text-align: center;
}
.pay-ps-box input:last-of-type{
  border-right: 1px solid #e5e5e5;
}
.btn-box{
  flex: 1;
  background: #fe5656;
  display: flex;
  border-radius: 5px;
  border-top: 1px solid #e5e5e5;
}
.btn-box button{
  flex: 1;
  font-size: 16px;
  background: #fff;
  line-height: 80rpx;
}
.btn-box button:first-of-type{
  border-radius: 0 0 0 5px;
  border-right: 1px solid #e5e5e5;
  color: #727272;
}

.btn-box button:last-of-type{
  border-radius: 0 0 5px 0;
  color: #fe5656;
}
.price-sum{
  margin-right: 30rpx;
}