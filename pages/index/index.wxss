/**index.wxss**/
swiper{
  height: 280rpx;
}
swiper image{
  width: 100%;
  height: 280rpx;
}
.kind-search-box{
  background: #fff;
  margin-top: 10rpx;
}
.home-item{
  background: #fff;
  margin-top: 25rpx;
}
/* 搜索定位 */
.search-addr{
  display: flex;
  line-height: 65rpx;
  padding: 10rpx 25rpx;
}
.addr-box{
  background: #e9e9e9;
  border-radius: 5px;
  color: #908b8b;
  display: flex;
  align-items: center;
  padding: 0 35rpx;
}
.search-box{
  padding: 0 20rpx;
  margin-left: 5rpx;
  border: 1px solid #aaaaaa;
  flex: 1;
  border-radius: 5px;
}
.search{
  display: flex;
  align-items: center;
}
.search .txt{
  color: #908b8b;
}
.addr-box .icon{
  width: 23rpx;
  height: 30rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  margin-right: 5rpx;
}
.search-box icon{
  line-height: 100%;
  margin-right: 5rpx;
}
.search-box input{
  flex: 1;
}
/* 分类 */
.kind-box{
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  /* justify-content: space-around; */
}
.kind-box navigator{
    width: 25%;
}
.kind-item{
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.kind-item image{
  width: 97rpx;
  height: 97rpx;
}
.kind-item text{
  padding: 10rpx 0;
  display: inline-block;
}

/* 品牌街,合作企业 */
.home-item .title{
  font-size: 16px;
  line-height: 100rpx;
  font-weight: bold;
  text-align: center;
  border-bottom: 1px solid #c9c9c9;
}
.home-joint-box .list,.home-brand-box .list{
  padding: 10rpx 0 30rpx 0;
  white-space: nowrap;
  display: flex;
  align-items: center;
}
.home-joint-box .item,.home-brand-box .item{
  position: relative;
  width: 200rpx;
  display: inline-block;
}
.home-joint-box .item::after,.home-brand-box .item::after{
  content: "";
  position: absolute;
  right: 0;
  top: 20%;
  bottom: 20%;
  width: 2px;
  background: #a0a0a0;
}
.home-joint-box .item:last-of-type:after,.home-brand-box .item:last-of-type:after{
  width: 0;
}
.home-joint-box image,.home-brand-box image{
  width: 118rpx;
  height: 118rpx;
  margin: 0 auto;
  display: block;
}

